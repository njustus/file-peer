<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<TitledPane fx:id="serverInfoView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Server" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="filepeer.ui.ClientDetailsController">
   <content>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox>
               <children>
                  <Label styleClass="server-details-description" text="Server" />
                  <Label fx:id="serverNameLbl" text="server" />
               </children>
            </HBox>
            <HBox layoutX="20.0" layoutY="20.0">
               <children>
                  <Label styleClass="server-details-description" text="IP" />
                  <Label fx:id="ipLbl" text="ip" />
               </children>
            </HBox>
            <HBox layoutX="20.0" layoutY="20.0">
               <children>
                  <Label styleClass="server-details-description" text="Last Connected" />
                  <Label fx:id="lastConnectedLbl" text="connected" />
               </children>
            </HBox>
            <HBox layoutX="20.0" layoutY="20.0">
               <children>
                  <Label styleClass="server-details-description" text="Shared Files" />
                  <Label fx:id="sharedFilesLbl" text="files" />
               </children>
            </HBox>
         </children>
      </VBox>
   </content>
</TitledPane>
